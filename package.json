{
  "name": "yahe",
  "version": "0.0.0",
  "description": "Yet Another Hints Extension",
  "main": "yahe.js",
  "private": true,
  "scripts": {
    "lint:check": "eslint options/*.js *.js",
    "lint:fix": "eslint --fix options/*.js *.js",
    "format:check": "prettier -c .",
    "format:fix": "prettier -w .",
    "fix:all": "npm run format:fix && npm run lint:fix",
    "check:all": "npm run format:check && npm run lint:check && npm run test",
    "test": "jest yahe.test.js",
    "build": "node build.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jpallari/yahe.git"
  },
  "author": "Jaakko Pallari",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/jpallari/yahe/issues"
  },
  "homepage": "https://github.com/jpallari/yahe#readme",
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "archiver": "^7.0.1",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "globals": "^15.13.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2"
  }
}
